<h1>Listing generic_network_devices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mac</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @generic_network_devices.each do |generic_network_device| %>
      <tr>
        <td><%= generic_network_device.name %></td>
        <td><%= generic_network_device.MAC %></td>
        <td><%= generic_network_device.description %></td>
        <td><%= link_to 'Show', generic_network_device %></td>
        <td><%= link_to 'Edit', edit_generic_network_device_path(generic_network_device) %></td>
        <td><%= link_to 'Destroy', generic_network_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Generic network device', new_generic_network_device_path %>
